<% display_model(system_user) do |s| %>
  <%= display_model_attribute(link_to_remote_with_load_notifier(s.name, :url => {:action => 'edit', :system_user_id => s.system_user_id})) %>
  <%= display_model_attribute(s.uid) %>
  <%= display_model_attribute('default group') %>
  <%= display_model_attribute(s.created_at) %>
  <%= display_model_attribute(s.updated_at) %>
<% end %>
