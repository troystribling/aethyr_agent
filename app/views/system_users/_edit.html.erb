<% display_control(@system_user) do |model| %>

  <%= display_header(:model => model) %>

  <% display_list(model) do |m| %>
    <%= display_list_attribute m, :name %>
    <%= display_list_attribute m, :uid %>
    <%= display_list_attribute 'default group', 'default group' %>
    <%= display_list_attribute m, :home_directory %>
    <%= display_list_attribute m, :login_shell %>
    <%= display_list_attribute m, :full_name %>
    <%= display_list_attribute m, :office %>
    <%= display_list_attribute m, :extension %>
    <%= display_list_attribute m, :home_phone %>
    <%= display_list_attribute m, :created_at %>
    <%= display_list_attribute m, :updated_at %>
  <% end %>  
  
  <%= display_buttons %>

  <%= display_table_of_models(:models => @system_groups, :partial => 'system_groups_table') %>

<% end %>  
